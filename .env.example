# Supabase Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

SUPABASE_URL=https://gfotskykmtedgjdvbqrh.supabase.co
SUPABASE_ANON_KEY=your_publishable_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Application Configuration
NODE_ENV=development
PORT=3000

# Analytics Configuration
GA_TRACKING_ID=your_ga_tracking_id
ENABLE_ANALYTICS=true

# Lead Scoring Configuration
HOT_LEAD_THRESHOLD=80
WARM_LEAD_THRESHOLD=60
QUALIFIED_LEAD_THRESHOLD=40

# Security Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production-min-32-characters
JWT_EXPIRES_IN=7d

# CORS Configuration (comma-separated origins)
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:5174

# Authentication Bypass (DEVELOPMENT ONLY - Set to 'true' to disable auth)
# WARNING: NEVER set to 'true' in production!
BYPASS_AUTH=false

# AI & Research API Keys
GROQ_API_KEY=your_groq_api_key_here
TAVILY_API_KEY=your_tavily_api_key_here
SERPER_API_KEY=your_serper_api_key_here
